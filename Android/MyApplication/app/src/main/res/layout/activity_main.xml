<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.zeppin.shebaoxiong.MainActivity">

    <cn.zeppin.shebaoxiong.TopBar
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        custom:leftImageDrawable="@drawable/back"
        custom:leftImageHeight="42dp"
        custom:leftImageWidth="42dp"
        custom:tittle="社保熊"
        custom:tittleBackground="@drawable/setbar_bg"
        custom:tittleTextColor="#4FACFE"
        custom:tittleTextSize="4.7dp" />


    <ProgressBar
        android:id="@+id/web_progress"
        android:layout_width="1dp"
        android:layout_height="44dp" />
    <View
        android:id="@+id/web_empty_view"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_height="0dp"/>
    <WebView
        android:id="@+id/webView"
        android:layout_below="@+id/web_progress"
        android:layout_above="@id/web_empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
         />
</RelativeLayout>