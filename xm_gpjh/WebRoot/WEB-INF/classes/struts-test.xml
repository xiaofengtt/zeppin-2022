<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts>
   <package name="test" extends="struts-default" namespace="/test">

		<global-results>
			<result name="grid">/test/grid.jsp</result>
		    <result name="json">/test/json.jsp</result>
		    <result name="gridjs">/test/gridjs.jsp</result>
		    <result name="excel">/test/export.xls</result>
		</global-results>

		<action name="myList_*" method="{1}" class="myListAction">
		</action>
		<action name="enumConst_*" method="{1}" class="enumConstAction">
		</action>
		<action name="systemVariables_*" method="{1}" class="systemVariablesAction">
		</action>
		<action name="systemApplay_*" method="{1}" class="systemApplayAction">
		</action>
	</package>
</struts>
