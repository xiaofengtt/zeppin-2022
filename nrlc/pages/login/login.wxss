/* login.wxss */
.top-view{
  width: 100%;
  height: 400rpx;
  background: #008DFB no-repeat center bottom url('data:image/png;base64,');
  background-size: 100% auto;
  text-align: center;
}

.top-view .logo{
  position: relative;
  width: 320rpx;
  height: 320rpx;
  margin: auto;
}

.top-view .logo image{
  width: 100%;
  height: 100%;
}

.top-view .logo text{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  color: rgba(255, 255, 255, 0.9);
  font-size: 32rpx;
  letter-spacing: 2rpx;
}

.main-view .tr:first-child{
  margin-top: 20rpx;
}

.main-view .tr{
  width: 100%;
  height: 100rpx;
}

.main-view .tr .data-row{
  position: relative;
  height: 100%;
  margin: 0 50rpx;
  border-bottom: 1px solid rgba(176, 176, 176, 0.3);
}

.main-view .tr .data-row image{
  position: absolute;
  left: 0;
  bottom: 10rpx;
  width: 34rpx;
  height: 38rpx;
}

.main-view .tr .data-row input{
  position: absolute;
  left: 50rpx;
  bottom: 0;
  height: 50rpx;
  width: 90%;
  font-size: 32rpx;
}

.main-view .tr .forget-row{
  position: relative;
  height: 100%;
  margin: 0 50rpx;
}

.main-view .tr .forget-row text{
  position: absolute;
  top: 0;
  right: 0;
  color: #008DFB;
  font-size: 28rpx;
  line-height: 100rpx;
}

.main-view .tr .submit-row{
  height: 100%;
  margin: 0 50rpx;
}

.main-view .tr .submit-row button{
  width: 100%;
  height: 100%;
  background: #008DFB;
  color: #FFF;
  line-height: 100rpx;
  font-size: 36rpx;
}

.foot-view{
  position: fixed;
  left: 0;
  bottom: 40rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #949494;
}

.foot-view text{
  color: #008DFB;
}