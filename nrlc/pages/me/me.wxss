/* me.wxss */
page{
  background: #eee;
}

.top-view{
  width: 100%;
  height: 250rpx;
  background: #008DFB no-repeat center bottom url('data:image/png;base64,');
  background-size: 100% auto; 
  text-align: center;
}

.top-view image{
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto 20rpx auto;
}

.top-view text{
  width: 100%;
  display: inline-block;
  color: #FFF;
  opacity: 0.8;
  font-size: 30rpx;
  line-height: 50rpx;
}

.top-view .button-view{
  width: 100%;
  height: 60rpx;
  text-align: center;
}

.top-view .button-view .button{
  display: inline-block;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 16rpx;
  color: #FFF;
  opacity: 0.8;
  border-radius: 6rpx;
  font-size: 30rpx;
  border: 1px solid #FFF;
}

.main-view{
  position: relative;
  width: 100%;
  height: 220rpx;
  background: #FFF;
}

.main-view .split{
  position: absolute;
  top: 60rpx;
  left: 50%;
  width: 1px;
  height: 100rpx;
  background: #EEE;
}

.main-view .content-view{
  display: inline-block;
  width: 50%;
  height: 100%;
  text-align: center;
}

.main-view .content-view image{
  width: 84rpx;
  height: 60rpx;
  margin: 40rpx auto 14rpx auto;
}

.main-view .content-view .title{
  margin-bottom: 4rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #000;
}

.main-view .content-view .message{
  line-height: 28rpx;
  font-size: 24rpx;
  color: #7C7C7C;
}

.main-view .content-view .message .light{
  color: #008DFB;
}

.table-view{
  width: 100%;
  margin-top: 26rpx;
}

.table-view .tr {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  margin-bottom: 1px;
  padding: 0 24rpx;
  background: #FFF;
}


.table-view .tr view{
  display: inline-block;
}

.table-view .tr .split{
  width: 6rpx;
  height: 28rpx;
  background: #8ACCFF;
}

.table-view .tr .text{
  position: absolute;
  width: 200rpx;
  margin-left: 10rpx;
  color: rgba(0, 0, 0, 0.8);
  font-size: 30rpx;
}


.table-view .tr image{
  position: absolute;
  top: 34rpx;
  right: 28rpx;
  width: 18rpx;
  height: 28rpx;
}

.logout-view{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #008DFB;
  font-size: 30rpx;
  margin-top: 40rpx;
  background: #FFF;
}

.border{
  position: fixed;
  height: 1px;
  width: 100%;
  bottom: 0;
  background-color: rgba(150, 150, 150, 0.4);
}